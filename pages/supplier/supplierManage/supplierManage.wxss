.headers{
  border: 1px solid #f9f6f6;
}
.headers .headers-item{
  background: rgba(236, 240, 247, 1);
  border-radius: 5px !important;
  padding-left: 5px !important;
}
.container{
  background: #fff;
  border: 1px solid #efefef;
  width: 96%;
  margin: 0px auto 10px;
  padding: 10px 5px;
  border-radius: 10px;
  position: relative;
  top: 1rem;
}
.container .headcalss{
  display: flex;
  padding-bottom: 10px;
  width: 100%
}
.container .headcalss .headleft{
  width: 70%;
}
.container .headcalss .headleft-item{
  padding: 2px 0px;
}
.container .headcalss .headright{
  display: flex;
  flex-direction: row-reverse;
  width: 30%;
}
.container .headcalss .headright-item{
  padding: 2px 10px;
  background:#1989FA;
  margin-left: 10px;
  color: #fff;
  border-radius: 9px;
}
.container .hrclass{
  background: rgba(236, 240, 247, 1);
  height: 1px;
  width: 96%;
  margin: 0 auto;
}
.container .footer{
  width: 100%;
  height: 100%;
  display: flex;
  padding: 5px 5px;
  font-size: 12px;
}
.container .footer-left{
  width: 55%;
}
.container .footer-left .left-item{
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
}
.container .footer-left .item1{
  margin: auto 0;
}
.container .footer-left .item2{
  padding: 5px 10px;
  background:#1989FA;
  margin-left: 10px;
  color: #fff;
  border-radius: 9px;
}
.container .footer-right{
  display: flex;
  width:45%;
  flex-direction: row-reverse;
}
.container .footer-right .right-item{
  font-size: 18px;
  color: #2121d8;
}
.footers{
  position: absolute;
  bottom: 2px;
  border: 1px solid #eaeaea;
  width: 100%;
  height: 50px;
  display: flex;
}
.footers .footers-item{
  border: 1px solid #88da96;
  margin: 0 auto; 
  background: #1989FA;
  font-size: 25px;
  padding: 5px 20px;
  border-radius: 11px;
  color: #fff;
  position: relative;
  top: 4px;
  margin-bottom: 3px;
}